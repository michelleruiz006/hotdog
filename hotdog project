num_of_ppl = int(input("Enter number of people attending: "))
num_of_hotdogs_prp = int(input("Enter number of hot dogs each will eat: "))

hotdogs_per_package = 10
buns_per_package = 8

total= num_of_ppl * num_of_hotdogs_prp

hotdog_packages_needed = total // hotdogs_per_package
hotdog_buns_needed = total // buns_per_package
hotdogs_left_over = total % hotdogs_per_package
buns_left_over = total % buns_per_package

print("Minimum number of packages of hot dogs required =", hotdog_packages_needed)
print("Minimum number of packages of hot dog buns required =", hotdog_buns_needed)
print("Number of hot dogs left over =", hotdogs_left_over)
print("Number of hot dogs buns left over =", buns_left_over)
